/*Write a program to find the Nth term of the Fibonnaci series.*/
#include<stdio.h>
int main()
{
int n;
int a=0;
int b=1;
int c=1;
// Input a number to find term of fibonacii series
printf("Enter the nth term which you want to see :");
scanf("%d",&n);
// iterate through n terms
for(int i=1;i<n;i++){
    c=a+b;
    a=b;
    b=c;
    if(i==n){
        break;
    }
}
printf("%d\t",c);



return 0;
}
